<form class="form-horizontal">

    <fieldset>
        <div class="form-group">
            <label class="col-md-2 control-label">Referece Number</label>
            <div class="col-md-6">
                <input class="form-control" disabled="disabled" placeholder="00000FXERAOA" type="text">
            </div>
        </div>
    </fieldset>

    <fieldset>
        <article class="col-sm-12 col-md-12 col-lg-12">
            <!-- Widget ID (each widget will need unique ID)-->
            <div class="jarviswidget" id="wid-id-2" data-widget-colorbutton="false" data-widget-editbutton="false">
                
                <!-- widget div-->
                <div>

                    <!-- widget edit box -->
                    <div class="jarviswidget-editbox">
                        <!-- This area used as dropdown edit box -->
                    </div>
                    <!-- end widget edit box -->
                    <!-- widget content -->
                    
                    <div class="widget-body fuelux">
                        <div class="widget-body-toolbar">

                        </div>
                        <table datatable="ng" id="dt_basic" class="table table-striped table-bordered table-hover" width="100%">
                            <thead>
                            <tr>
                                <th>Event Number</th>
                                <th>Event Code</th>
                                <th>Description</th>
                                <th>Event Date</th>
                                <th>Authrization Status</th>
                                <th>Maker Id </th>
                                <th class="text-center">Action </th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr ng-repeat="event in EventsList">
                                <td>{{event.EventNumber}}</td>
                                <td>{{event.EventCode}}</td>
                                <td>{{event.Description}}</td>
                                <td>{{event.EventDate}}</td>
                                <td>{{event.AuthrizationStatus}}</td>
                                <td>{{event.MakerId }}</td>
                                <td>
                                    <a data-toggle="modal" data-target="#editEvent">
                                        <i class="fa fa-pencil-square"></i>&nbsp;Edit
                                    </a>
                                    <a><i class="fa fa-minus-square"></i>&nbsp;Delete</a>
                                    <a href="#/assets/manageEvents"><i class="fa fa-external-link-square"></i>&nbsp;Manage Events</a>
                                </td>
                            </tr>
                            </tbody>
                        </table>

                        <div saving-process > </div>
                    </div>

                  
                    <!-- end widget content -->

                </div>
                <!-- end widget div -->

            </div>
            <!-- end widget -->

        </article>
        <!-- WIDGET END -->
    </fieldset>

</form>




<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
