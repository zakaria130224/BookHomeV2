<body ng-app="EnterpriseSolution" class="fixed-header fixed-navigation fixed-page-footer" style="background-color: #F1F1F1;">
    <!-- HEADER -->

    <header id="header">
        <div id="logo-group">
            <!-- PLACE YOUR LOGO HERE -->
            <span id="logo"> <img src="~/Content/img/logo.png" alt="APPAREL ERP SUITE"> </span>
            <!-- collapse menu button -->
            <div id="hide-menu" class="btn-header">
                <span> <a href="javascript:void(0);" data-action="toggleMenu" title="Collapse Menu" style="height:25px!important"><i class="fa fa-reorder"></i></a> </span>
            </div>
            <!-- end collapse menu -->
        </div>

        <!-- pulled right: nav area -->
        <div class="hidden-xs padding-top-15">
            <div class="btn-header pull-left text-uppercase padding-right-25">
                <span> <b>Browser:</b> @ViewBag.browserName</span>
            </div>
            <div class="btn-header pull-left text-uppercase">
                <span> <b>IP Address:</b> @ViewBag.ipAddress</span>
            </div>
            <div id="loading-bar-container"></div>
        </div>

        <div class="pull-right">


            <!-- #MOBILE -->
            <!-- Top menu profile link : this shows only when top menu is active -->
            <ul id="mobile-profile-img" class="header-dropdown-list hidden-xs padding-5">
                <li class="">
                    <a href="#" class="dropdown-toggle no-margin userdropdown" data-toggle="dropdown">
                        <img src="~/Content/img/avatars/sunny.png" alt="John Doe" class="online" />
                    </a>
                    <ul class="dropdown-menu pull-right">
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0"><i class="fa fa-cog"></i> Setting</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="profile.html" class="padding-10 padding-top-0 padding-bottom-0"> <i class="fa fa-user"></i> <u>P</u>rofile</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0" data-action="toggleShortcut"><i class="fa fa-arrow-down"></i> <u>S</u>hortcut</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0" data-action="launchFullscreen"><i class="fa fa-arrows-alt"></i> Full <u>S</u>creen</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="login.html" class="padding-10 padding-top-5 padding-bottom-5" data-action="userLogout"><i class="fa fa-sign-out fa-lg"></i> <strong><u>L</u>ogout</strong></a>
                        </li>
                    </ul>
                </li>
            </ul>

            <!-- logout button -->
            <div id="logout" class="btn-header transparent pull-right" ng-controller="LoginCtrl">
                <span> <a ng-click="logout()" title="Logout">&nbsp;Log out&nbsp;<i class="fa fa-sign-out"></i>&nbsp;</a> </span>

            </div>
            <!-- end logout button -->

            <div id="search-mobile" class="btn-header transparent pull-right">
                <span> <a href="javascript:void(0)" title="Search"><i class="fa fa-search"></i></a> </span>
            </div>
            <!-- end search mobile button -->
            <!-- fullscreen button -->
            <div id="fullscreen" class="btn-header transparent pull-right">
                <span> <a href="javascript:void(0);" data-action="launchFullscreen" title="Full Screen"><i class="fa fa-arrows-alt"></i></a> </span>
            </div>
            <!-- end fullscreen button -->

        </div>
        <!-- end pulled right: nav area -->

    </header>
    <!-- END HEADER -->
    <!-- Left panel : Navigation area -->
    <!-- Note: This width of the aside area can be adjusted through LESS variables -->
    <aside id="left-panel">

        <!-- User info -->
        <div class="login-info">
            <span>
                <!-- User image size is adjusted inside CSS, it should stay as it -->

                <a href="javascript:void(0);" id="show-shortcut" data-action="toggleShortcut">
                    <img src="~/Content/img/avatars/male.png" alt="me" class="online" />
                    <span>
                        @ViewBag.LoggedInUserName
                    </span>
                    @*<i class="fa fa-angle-down"></i>*@
                </a>

            </span>
        </div>
        <!-- end user info -->
        <!-- NAVIGATION : This navigation is also responsive-->
        @Html.Action("NavBar", "Application");
        <span class="minifyme" data-action="minifyMenu">
            <i class="fa fa-arrow-circle-left hit"></i>
        </span>

    </aside>
    <!-- END NAVIGATION -->
    <!-- MAIN PANEL -->
    <div id="main" role="main" style="min-height:830px !important">
        <!-- MAIN CONTENT -->
        <div id="content">
            @RenderBody()
            <div flash-message="10000" id="flash-message"></div>

        </div>
        <!-- END MAIN CONTENT -->

    </div>
    <!-- END MAIN PANEL -->
    <!-- PAGE FOOTER -->
    <div class="page-footer">
        <div class="row">
            <div class="col-xs-12 col-sm-6">
                <span class="txt-color-white">
                    Enterprise Solution<span class="hidden-xs"> - Copyright</span> © 2017-2018, Developed By
                    <b>CheesecakeICT</b>
                </span>
            </div>

            <div class="col-xs-6 col-sm-6 text-right hidden-xs">
                <div class="txt-color-white inline-block">
                    <i class="txt-color-blueLight hidden-mobile">Last account activity <i class="fa fa-clock-o"></i> <strong>52 mins ago &nbsp;</strong> </i>
                </div>
            </div>
        </div>
    </div>
    <div ng-controller="masterCtrl">
        <session-out-modal unique-id="sessionOut" count-down="countDown" method="DeclineSession()"></session-out-modal>
    </div>
    <!-- END PAGE FOOTER -->
    <!--Bundale Load-->
    @Scripts.Render("~/bundles/masterlayoutthemejs")

    <!-- Angular Js Files -->

    @Scripts.Render("~/bundles/masterlayoutangularjs")



    <script src="~/Scripts/angularjs/angular-datatables.js"></script>
    <script src="~/Scripts/angular/directive/angular-table/angular-table.min.js"></script>
    <script src="~/Scripts/loadingbar/loading-bar.js"></script>
    <script src="~/Scripts/angular-toastr.js"></script>
    <script src="~/Scripts/angular/directive/tree-grid/tree-grid-directive.js"></script>
    <script src="~/Scripts/angular/directive/angucomplete.js"></script>
    <script src="~/Scripts/angular/directive/angular-confirm.min.js"></script>
    <script src="~/Content/ckeditor/ckeditor.js"></script>

    <script src="~/Scripts/angular/app.js"></script>

    <script src="~/Scripts/angular/directive/custom.js"></script>

    @Scripts.Render("~/bundles/masterlayoutngcustomdirectivesres2js")


    @Scripts.Render("~/bundles/masterlayoutngcustomdirectivesresjs")

    @Scripts.Render("~/bundles/masterlayoutAngularServicejs")

    <!-- Angular Custom Controller -->
    <script src="~/Scripts/angular/controllers/login-controller.js"></script>
    <script src="~/Scripts/angular/controllers/DashboardController.js"></script>
    <script src="~/Scripts/angular/controllers/UserCntroller.js"></script>
    <script src="~/Scripts/angular/controllers/MasterController.js"></script>
    <script src="~/Scripts/angular/controllers/BookCategoryController.js"></script>
        <script src="~/Scripts/angular/controllers/BookController.js"></script>
        <script src="~/Scripts/angular/controllers/AuthorController.js"></script>
        <script src="~/Scripts/angular/controllers/BranchController.js"></script>
        <script src="~/Scripts/angular/controllers/UserRoleController.js"></script>

    @*@Scripts.Render("~/bundles/masterlayoutangularcustomcontrollerjs")*@

    <!-- Angular flash -->
    <script src="~/Scripts/angular-flash.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
